{
  "name": "jar-client-x",
  "productName": "jar-client-x",
  "version": "1.0.1",
  "description": "Java Applet Replacement Client Side Application",
  "main": "app.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "install-app-deps",
    "pack": "build --dir",
    "dist": "build"
  },
  "repository": "https://github.com/XEonAX/jar-client/",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": {
    "name": "Sumant Vanage"
  },
  "license": "MIT",
  "build": {
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "build/",
      "category": "Network"
    },
    "win": {
      "target": [
        "nsis",
        "nsis-web",
        "portable"
      ],
      "icon": "build/icon.ico"
    }
  },
  "devDependencies": {
    "electron": "~1.7.8",
    "electron-builder": "^19.37.2"
  },
  "dependencies": {
    "electron-clipboard-watcher": "^1.0.1",
    "electron-debug": "^1.4.0",
    "https-proxy-agent": "^2.1.0",
    "jdenticon": "^1.7.2",
    "node-notifier": "^5.1.2",
    "systeminformation": "^3.30.6",
    "ws": "^3.2.0"
  }
}
